
function fibonacciGenerator (n) {

   
    let collection=[];
    let i=0;
    let j=1;
    if(n===1){
       return [0];
    }
    if(n===2){
        return [0,1];
    }
    collection.push(0);
    collection.push(1);
    while(n!==collection.length){
             collection.push(collection[i]+collection[j]);
             
             i++;
             j++;
        }
return collection;

}

